<?xml version="1.0"?>
<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/layout_generic.xsd">
    <container name="root">
        <block class="MX\WidgetComponent\Block\Adminhtml\Component\ImagePicker\Content" name="wysiwyg_images.content" template="Magento_Cms::browser/content.phtml">
            <block class="MX\WidgetComponent\Block\Adminhtml\Component\ImagePicker\Tree" name="wysiwyg_images.tree" template="Magento_Cms::browser/tree.phtml"/>
            <block class="MX\WidgetComponent\Block\Adminhtml\Component\ImagePicker\Uploader" name="wysiwyg_images.uploader" template="Magento_Cms::browser/content/uploader.phtml">
                <arguments>
                    <argument name="image_upload_config_data" xsi:type="object">Magento\Backend\Block\DataProviders\ImageUploadConfig</argument>
                </arguments>
            </block>
        </block>
    </container>
</layout>
